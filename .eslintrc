{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint-config-airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "react/prefer-stateless-function": 0,
    "react/no-multi-comp": 0,
    "react/prop-types": 0
  }
}